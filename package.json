{
  "name": "@antd-templater/antd-template-lib3.x",
  "description": "基于 Vue3.x Ant Design Vue 对Tree、Table、Form等组件二次封装",
  "homepage": "https://github.com/antd-templater/antd-template-lib3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "version": "0.0.2",
  "keywords": [
    "front-end",
    "vue-components",
    "ant-design-vue"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./util/*": {
      "types": "./dist/util/*.d.ts",
      "import": "./dist/util/*.mjs",
      "require": "./dist/util/*.cjs"
    },
    "./*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/antd-template-lib3.x.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/antd-template-lib3.x/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "shx rm -rf dist && rollup --config rollup.config.ts --configPlugin typescript",
    "eslint": "npx eslint --fix --quiet src --ext .vue,.js,ts,jsx,tsx"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.15",
    "dayjs": "^1.11.7",
    "less": "^4.1.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/pluginutils": "^5.0.2",
    "@types/eslint": "^8.21.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript-eslint/typescript-estree": "^5.51.0",
    "@volar-plugins/prettyhtml": "^1.0.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47",
    "cz-message-helper": "^1.2.1",
    "eslint": "^8.33.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rollup": "^3.14.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.24"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}