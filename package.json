{
  "name": "@antd-templater/antd-template-lib3.x",
  "description": "基于 Vue3.x Ant Design Vue 对Tree、Table、Form等组件二次封装",
  "homepage": "https://github.com/antd-templater/antd-template-lib3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "version": "0.0.23",
  "keywords": [
    "front-end",
    "vue-components",
    "ant-design-vue"
  ],
  "files": [
    "dist",
    "library-declare.d.ts",
    "library-eslintrc.json"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/antd-template-lib3.x.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/antd-template-lib3.x/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.20",
    "dayjs": "^1.11.7",
    "less": "^4.1.3",
    "vue": "^3.3.4",
    "vue-types": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/pluginutils": "^5.0.2",
    "@types/eslint": "^8.40.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/typescript-estree": "^5.59.8",
    "@volar-plugins/prettyhtml": "^2.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.3.4",
    "cz-message-helper": "^1.2.2",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.6.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "shx rm -rf dist && rollup --config rollup.config.ts --configPlugin typescript",
    "eslint": "npx eslint --fix --quiet src --ext .vue,.js,ts,jsx,tsx"
  }
}