{
  "name": "@antd-templater/antd-template-lib3.x",
  "description": "基于 Vue3.x Ant Design Vue 对Tree、Table、Form等组件二次封装",
  "homepage": "https://github.com/antd-templater/antd-template-lib3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "form",
    "tree",
    "table",
    "front-end",
    "ant-design-vue",
    "vue-components"
  ],
  "files": [
    "dist",
    "src/example",
    "library-declare.d.ts",
    "library-eslintrc.json",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/antd-template-lib3.x.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/antd-template-lib3.x/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "ant-design-vue": "^3.2.20",
    "dayjs": "^1.11.9",
    "less": "^4.1.3",
    "vue": "^3.3.4",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@release-it/conventional-changelog": "^7.0.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/pluginutils": "^5.0.2",
    "@types/eslint": "^8.44.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@typescript-eslint/typescript-estree": "^6.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.3.4",
    "cz-message-helper": "^1.3.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "rollup": "^3.27.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-vue": "^6.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0",
    "volar-service-prettyhtml": "^0.0.11",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.8.8"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "vite": "shx rm -rf dist && vue-tsc --noEmit && vite build",
    "rollup": "shx rm -rf dist && rollup --config rollup.config.ts --configPlugin typescript",
    "eslint": "npx eslint --fix --quiet src --ext .vue,.js,ts,jsx,tsx",
    "release": "release-it"
  }
}
